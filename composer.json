{
    "name": "laravel-global-search/global-search",
    "description": "A comprehensive Laravel package for global search functionality with Meilisearch integration, multi-tenancy support, and advanced features.",
    "keywords": ["laravel", "search", "meilisearch", "global-search", "multi-tenant", "analytics"],
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.3",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/contracts": "^10.0|^11.0|^12.0",
        "illuminate/console": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/cache": "^10.0|^11.0|^12.0",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/queue": "^10.0|^11.0|^12.0",
        "meilisearch/meilisearch-php": "^1.0",
        "guzzlehttp/guzzle": "^7.0",
        "ext-json": "*",
        "ext-curl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0|^9.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelGlobalSearch\\GlobalSearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelGlobalSearch\\GlobalSearch\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test-coverage": "phpunit --coverage-html coverage"
    },
        "extra": {
        "laravel": {
            "providers": [
                "LaravelGlobalSearch\\GlobalSearch\\GlobalSearchServiceProvider"
            ],
            "aliases": {
                "GlobalSearch": "LaravelGlobalSearch\\GlobalSearch\\Services\\GlobalSearchService"
            }
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}