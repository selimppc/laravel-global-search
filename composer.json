{
    "name": "selimppc/laravel-global-search",
    "description": "Config-first federated global search for Laravel powered by Meilisearch.",
    "type": "library",
    "license": "MIT",
    "autoload": {
    "psr-4": {
        "Selimppc\\GlobalSearch\\": "src/"
    }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Selimppc\\GlobalSearch\\GlobalSearchServiceProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/queue": "^10.0|^11.0|^12.0",
        "illuminate/cache": "^10.0|^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}